<div class="my-workouts-main">
    <div class="my-workouts-headline">
        <h1>All Workouts</h1>
    </div>
    {{#each workouts as |workout|}}
    <div class="my-workout-card">
        <h2>{{workout.title}}</h2>
        <p>{{workout.description}}</p>
        <p>{{workout.rating}}</p>
        <p>{{workout.body_zone}}</p>
        <p>{{workout.duration}}</p>
    </div>
    {{/each}}
</div>